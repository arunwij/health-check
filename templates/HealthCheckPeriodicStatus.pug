    width: 58.3% !important;
    }
    .no-stack .col.num8 {
    width: 66.6% !important;
    }
    .no-stack .col.num9 {
    width: 75% !important;
    }
    .no-stack .col.num10 {
    width: 83.3% !important;
    }
    .video-block {
    max-width: none !important;
    }
    .mobile_hide {
    min-height: 0px;
    max-height: 0px;
    max-width: 0px;
    display: none;
    overflow: hidden;
    font-size: 0px;
    }
    .desktop_hide {
    display: block !important;
    max-height: none !important;
    }
    .img-container.big img {
    width: auto !important;
    }
